<One>:
    md_bg_color: [0.8, 0.8, 0.8, 1]

    MDLabel:
        id: label_one
        text: "SCREEN ONE"
        font_style: 'H4'
        halign: 'center'
        pos_hint: {'center_y': 0.6}

    MDLabel:
        id: label_two
        text: "This is screen one 33"
        halign: 'center'

    MDTextField:
        id: data
        text: "one"
        pos_hint: {'center_x': 0.5, 'center_y': 0.4}
        size_hint_x: 0.5

    MDRoundFlatButton:
        id: change_screen
        text: "Go to screen 2"
        md_bg_color: [1, 1, 1, 1]
        pos_hint: {'center_x': 0.5, 'center_y': 0.3}
        on_release: app.sm.current = 'two'

